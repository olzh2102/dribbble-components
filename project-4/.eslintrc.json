{
  "plugins": ["@typescript-eslint"],
  "extends": [
    "next/babel",
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "prefer-const": "warn",
    "@typescript-eslint/no-explicit-any": "warn",
    "no-console": "warn",
    "max-len": ["warn", { "code": 80 }]
  }
}